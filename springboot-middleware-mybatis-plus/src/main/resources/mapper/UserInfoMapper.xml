<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.feifuzeng.middleware.mp.dao.UserInfoDao">
    <select id="selectUserInfoByGtFraction" resultType="com.github.feifuzeng.middleware.mp.entity.UserInfoEntity" parameterType="long">
        SELECT * FROM user_info WHERE fraction > #{fraction}
    </select>
</mapper>